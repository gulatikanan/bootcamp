{
  "processors": [
    {
      "type": "filter",
      "min_length": 5,
      "pattern": "^[a-zA-Z]"
    },
    {
      "type": "line_count",
      "pattern": "Line #{count}: {line}",
      "start_count": 1
    },
    {
      "type": "transform",
      "transform_func": "lambda s: s.upper()"
    },
    {
      "type": "split",
      "delimiter": ","
    },
    {
      "type": "join",
      "count": 2,
      "delimiter": " | "
    }
  ]
}